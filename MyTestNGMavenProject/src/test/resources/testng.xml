<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="TestNG">
    <test name="Login Test">
        <classes>
            <class name="FacebookLoginTest"></class>
        </classes>
    </test>
    <test name="Profile Test">
        <classes>
            <class name="FacebookProfileTest"></class>
        </classes>
    </test>
    <test name="Package Test Run">
        <packages>
            <package name="com.testng.training"></package>
        </packages>
    </test>
    <test name="Annotations Test">
        <classes>
            <class name="executionorder.SampleTestNGUsage"></class>
            <class name="executionorder.AnnotationsTest"></class>
        </classes>
    </test>
    <test name="Params Test">
        <parameter name="username" value="rumite"></parameter>
        <parameter name="password" value="QAwsDrf"></parameter>
        <classes>
            <class name="ParametrizedTest"></class>
        </classes>
    </test>
</suite>